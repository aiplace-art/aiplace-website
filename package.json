{
  "name": "website-development-team",
  "version": "2.0.0",
  "description": "Professional team of 9 specialists for website development with Claude Flow orchestration",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "init:team": "./scripts/init-team.sh",
    "validate:structure": "node scripts/validate-structure.js",
    "generate:structure-map": "node scripts/generate-structure-map.js",
    "fix:structure": "node scripts/validate-structure.js --fix",
    "swarm:init": "npx claude-flow@alpha swarm init --topology hierarchical --max-agents 9",
    "swarm:status": "npx claude-flow@alpha swarm status",
    "agent:metrics": "npx claude-flow@alpha agent metrics",
    "task:status": "npx claude-flow@alpha task status",
    "memory:usage": "npx claude-flow@alpha memory usage",
    "benchmark": "npx claude-flow@alpha benchmark run",
    "audit:team": "node scripts/validate-structure.js && echo 'âœ… Team audit complete'",
    "docs:generate": "npm run generate:structure-map",
    "precommit": "npm run validate:structure",
    "test": "npm run test:all",
    "test:layout": "node scripts/test-layout.js",
    "test:i18n": "node scripts/test-i18n.js",
    "test:visual": "playwright test",
    "test:performance": "node scripts/test-performance.js",
    "test:accessibility": "node scripts/test-accessibility.js",
    "test:all": "npm run test:layout && npm run test:i18n && npm run test:visual",
    "test:watch": "nodemon --watch src --exec 'npm run test:quick'",
    "test:quick": "npm run test:layout && npm run test:i18n",
    "test:report": "node scripts/generate-report.js",
    "test:ci": "npm run test:layout && npm run test:i18n && npm run test:visual -- --reporter=json",
    "help": "echo 'Available commands:' && npm run"
  },
  "keywords": [
    "website-development",
    "claude-flow",
    "orchestration",
    "swarm",
    "react",
    "nextjs",
    "full-stack",
    "devops",
    "automation"
  ],
  "author": "Website Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.3",
    "lucide-react": "^0.344.0",
    "nanoid": "^5.1.6",
    "next": "^14.1.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.63.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.6",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.56.0",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "claude-flow": "^2.0.0-alpha",
    "cssnano": "^7.1.1",
    "lighthouse": "^12.8.2",
    "nodemon": "^3.1.10",
    "pa11y": "^9.0.1",
    "playwright": "^1.55.1",
    "postcss": "^8.4.35",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/website-team"
  }
}
